mirrors:
  eicweb:
    autopush: true
    url: oci://${CI_REGISTRY}/${CI_PROJECT_PATH}/spack-0.23.0
    signed: false
    fetch:
      access_pair:
      - ${CI_REGISTRY_USER}
      - ${CI_REGISTRY_PASSWORD}
    push:
      access_pair:
      - ${CI_REGISTRY_USER}
      - ${CI_REGISTRY_PASSWORD}
  ghcr:
    autopush: true
    url: oci://ghcr.io/eic/spack-0.23.0
    signed: false
    fetch:
      access_pair:
      - ${GITHUB_REGISTRY_USER}
      - ${GITHUB_REGISTRY_TOKEN}
    push:
      access_pair:
      - ${GITHUB_REGISTRY_USER}
      - ${GITHUB_REGISTRY_TOKEN}
